{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "site": "simplq-us",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
      }]
  }
}
